def split (s : String) : List String :=
  s.splitOn "\n"

inductive Command
| Forward
| Down
| Up

def Read : String → Command
| "forward" => Command.Forward
| "down" => Command.Down
| "up" => Command.Up
| _ => Command.Forward

def inputs (s: String) : Command × Int :=
match String.splitOn s with
| [cmd, num] => (Read cmd, String.toInt! num)
| _ => (Command.Forward, 0)

def coordinatePair (aim : Int ): List (Command × Int) → Int × Int
| [] => (0,0)
| ((Command.Down, n) :: xs) => coordinatePair (aim + n) xs
| ((Command.Up, n) :: xs) => coordinatePair (aim - n) xs
| ((Command.Forward, n) :: xs) => let (x, y) := coordinatePair (aim) xs
       (x + (aim * n), y + n)

def commandlist :=  (split test).map inputs

def depth:= coordinatePair 0 (commandlist)

def pair := depth

def mult :=
let x:= pair.1
let y := pair.2
x*y

#eval pair #eval mult